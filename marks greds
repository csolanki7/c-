#include <iostream>

using namespace std; 

int main() {
	int num; 

	 cout <<  " Enter your mark " << endl;
	 cin >> num ;

	if (num  >= 90 && num  <= 100)  {

		cout << " student hase scored A grade " << endl; 
	}
	else if (num >= 80 && num <= 89) {
		cout << " student hase scored B grade " << endl; 
	}
	else if (num >= 70 && num <= 79) {
		cout <<  " student hase scored C grade "  << endl; 
	}

	else if (num < 60  ) {
		cout << " student Failed in this exam" << endl;
	}

	else if ( num > 100 && num < 0 ) {
       cout << " error " << endl;
	}
	system(" pause "); 
	return 0;
}

